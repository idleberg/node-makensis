{
	"name": "makensis",
	"version": "3.0.0-alpha.5",
	"description": "A Node wrapper for makensis, the NSIS compiler",
	"type": "module",
	"module": "./dist/makensis.js",
	"types": "./dist/makensis.d.ts",
	"exports": {
		".": {
			"import": "./dist/makensis.js",
			"types": "./dist/makensis.d.ts"
		}
	},
	"files": [
		"dist/",
		"LICENSE",
		"README.md"
	],
	"engines": {
		"node": "^18.0.0 || ^20.0.0 || >=22.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/node-makensis.git"
	},
	"keywords": [
		"nsis",
		"makensis"
	],
	"license": "MIT",
	"dependencies": {
		"@nsis/language-data": "^0.9.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.4",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@types/node": "^22.15.21",
		"jsr": "^0.13.4",
		"lefthook": "^1.11.14",
		"np": "^10.2.0",
		"rimraf": "^6.0.1",
		"tslib": "^2.8.1",
		"tsm": "^2.3.0",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3",
		"uvu": "^0.5.6",
		"which": "^5.0.0"
	},
	"scripts": {
		"build": "tsup",
		"dev": "npm run build -- --watch",
		"lint": "biome check --config-path=biome.jsonc --no-errors-on-unmatched --vcs-enabled=true --vcs-use-ignore-file=true",
		"prepack": "npm run build",
		"prepare": "lefthook install",
		"prepublishOnly": "npm run lint && npm run test",
		"publish:jsr": "jsr publish",
		"publish:npm": "np --any-branch",
		"test": "uvu tests -r tsm --ignore shared.ts"
	},
	"packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
