{
	"name": "makensis",
	"version": "3.0.0-alpha.3",
	"description": "A Node wrapper for makensis, the NSIS compiler",
	"type": "module",
	"module": "./dist/makensis.js",
	"types": "./dist/makensis.d.ts",
	"exports": {
		".": {
			"import": "./dist/makensis.js",
			"types": "./dist/makensis.d.ts"
		}
	},
	"files": [
		"dist/",
		"LICENSE",
		"README.md"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/node-makensis.git"
	},
	"keywords": [
		"nsis",
		"makensis"
	],
	"license": "MIT",
	"dependencies": {
		"@nsis/language-data": "^0.9.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@types/node": "^18.19.64",
		"concurrently": "^9.1.0",
		"jsr": "^0.12.4",
		"lefthook": "^1.11.13",
		"np": "^10.0.7",
		"remark-cli": "^12.0.1",
		"remark-preset-lint-recommended": "^6.1.3",
		"remark-preset-prettier": "^2.0.1",
		"rimraf": "^5.0.10",
		"tslib": "^2.8.1",
		"tsm": "^2.3.0",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"uvu": "^0.5.6",
		"which": "^4.0.0"
	},
	"scripts": {
		"build": "tsup",
		"dev": "npm run build -- --watch",
		"fix": "eslint --fix ./src",
		"lint:code": "biome check --config-path=biome.jsonc --no-errors-on-unmatched --vcs-enabled=true --vcs-use-ignore-file=true",
		"lint:md": "remark . --quiet --frail --ignore-path .gitignore",
		"lint": "concurrently --prefix '{name}' -c 'blue,green' 'npm:lint:*'",
		"prepack": "npm run build",
		"prepare": "lefthook install",
		"prepublishOnly": "npm run lint && npm run test",
		"publish:jsr": "jsr publish",
		"publish:npm": "np --no-yarn --any-branch",
		"test": "uvu tests -r tsm --ignore shared.ts"
	},
	"packageManager": "pnpm@8.15.4+sha256.cea6d0bdf2de3a0549582da3983c70c92ffc577ff4410cbf190817ddc35137c2"
}
