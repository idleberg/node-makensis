{
  "name": "makensis",
  "version": "0.21.2",
  "description": "A Node wrapper for makensis, the NSIS compiler",
  "main": "dist/makensis.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-makensis.git"
  },
  "keywords": [
    "nsis",
    "makensis"
  ],
  "license": "MIT",
  "dependencies": {
    "@nsis/language-data": "^0.7.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "ava": "^3.12.1",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run start",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prepublishOnly": "npm run lint && npm run build",
    "start": "rollup -w -c",
    "test": "ava ./test/commands.js ./test/encoding.js --verbose",
    "test:wine": "ava ./test/wine.js --verbose --timeout 2m"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
