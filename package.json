{
  "name": "makensis",
  "version": "0.18.2",
  "description": "A Node wrapper for makensis, the NSIS compiler",
  "main": "dist/makensis.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-makensis.git"
  },
  "keywords": [
    "nsis",
    "makensis"
  ],
  "license": "MIT",
  "dependencies": {
    "@nsis/language-data": "^0.7.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.11",
    "ava": "^2.4.0",
    "husky": "^3.1.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.2",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "ava ./test/commands.js ./test/encoding.js --verbose",
    "test:wine": "ava ./test/wine.js --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
