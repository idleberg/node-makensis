{
  "name": "makensis",
  "version": "0.17.0",
  "description": "A Node wrapper for makensis, the NSIS compiler",
  "main": "dist/makensis.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-makensis.git"
  },
  "keywords": [
    "nsis",
    "makensis"
  ],
  "license": "MIT",
  "dependencies": {
    "@nsis/language-data": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.26",
    "ava": "^1.2.1",
    "gulp": "^4.0.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.2.2",
    "gulp-tslint": "^8.1.3",
    "husky": "^1.3.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project .",
    "prepublishOnly": "gulp lint && npm run build",
    "test": "npx ava ./test/default.js --verbose",
    "test:encoding": "npx ava ./test/encoding.js --verbose",
    "test:wine": "npx ava ./test/wine.js --verbose"
  }
}
